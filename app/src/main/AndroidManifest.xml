<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.git_foodtracker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/qseu_icon_kuehlschrank"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewholder_product"></activity>
        <activity android:name=".bestand_recyclerview" />
        <activity android:name=".get_product" />
        <activity android:name=".add_product"
            android:exported="true"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>